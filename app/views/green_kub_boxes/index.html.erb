<p id="notice"><%= notice %></p>

<h1>Listing Green Kub Boxes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @green_kub_boxes.each do |green_kub_box| %>
      <tr>
        <td><%= link_to 'Show', green_kub_box %></td>
        <td><%= link_to 'Edit', edit_green_kub_box_path(green_kub_box) %></td>
        <td><%= link_to 'Destroy', green_kub_box, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Green kub box', new_green_kub_box_path %>
