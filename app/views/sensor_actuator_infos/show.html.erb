<%- model_class = SensorActuatorInfo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:value) %>:</strong></dt>
  <dd><%= @sensor_actuator_info.value %></dd>
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= @sensor_actuator_info.date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sensor_id) %>:</strong></dt>
  <dd><%= @sensor_actuator_info.sensor_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              sensor_actuator_infos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_sensor_actuator_info_path(@sensor_actuator_info), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              sensor_actuator_info_path(@sensor_actuator_info),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
