<%- model_class = SensorActuatorInfo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:value) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:sensor_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @sensor_actuator_infos.each do |sensor_actuator_info| %>
      <tr>
        <td><%= link_to sensor_actuator_info.id, sensor_actuator_info_path(sensor_actuator_info) %></td>
        <td><%= sensor_actuator_info.value %></td>
        <td><%= sensor_actuator_info.date %></td>
        <td><%= sensor_actuator_info.sensor_id %></td>
        <td><%=l sensor_actuator_info.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_sensor_actuator_info_path(sensor_actuator_info), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      sensor_actuator_info_path(sensor_actuator_info),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_sensor_actuator_info_path,
            :class => 'btn btn-primary' %>
